<odoo>
  <data>

    <record id="view_move_form" model="ir.ui.view">
      <field name="name">account.move.custom.form</field>
      <field name="model">account.move</field>
      <field name="inherit_id" ref="account.view_move_form"/>
      <field name="arch" type="xml">
        <group id="other_tab_group" position="inside">
          <group string="Vendor Period of Billing">
            <field name="period_from_date" readonly="1" force_save="1"/>
            <field name="period_to_date" readonly="1" force_save="1"/>
          </group>
        </group>
      </field>
    </record>

  </data>
</odoo>